{
    "compilerOptions": {
        "target": "es2022",
        "lib": ["esnext"],
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "allowImportingTsExtensions": true,
        "noUnusedLocals": true,
        "strict": true,
        "newLine": "lf"
    },
    "include": ["*.ts"]
}
